    "validate": "npm run style && npm run coverage"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "nyc": "^15.1.0"
  }
}
