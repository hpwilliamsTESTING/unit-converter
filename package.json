{
  "name": "unit-converter",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "init": "eslint --init",
    "style": "eslint lib/*.js ",
